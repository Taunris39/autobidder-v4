    "mailparser": "^3.7.5",
    "open": "^11.0.0",
    "p-retry": "^7.1.1",
    "prettier": "^3.7.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.4",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
