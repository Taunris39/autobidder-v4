{
  "name": "autobidder",
  "version": "4.0.0",
  "description": "Created by Jonas Mikkelsen to automate bidding on loads in Telegram groups.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "watch": "nodemon --watch src --exec \"node --loader ts-node/esm src/index.ts\""
  },
  "dependencies": {
    "@google-cloud/pubsub": "^5.2.0",
    "dotenv": "^17.2.3",
    "googleapis": "^169.0.0",
    "grammy": "^1.39.2",
    "mailparser": "^3.9.1",
    "open": "^11.0.0",
    "p-retry": "^7.1.1"
  },
  "devDependencies": {
    "@types/mailparser": "^3.4.6",
    "@types/node": "^25.0.3",
    "globals": "^17.0.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "engines": {
    "node": ">=18"
  },
